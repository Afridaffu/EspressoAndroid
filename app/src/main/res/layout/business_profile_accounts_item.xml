<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_group_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/profileLL"
    android:background="@color/white"
    android:orientation="vertical"
    android:visibility="visible">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_6ssp"
        android:background="#F6F9FE"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/_9sdp"
        android:layout_marginStart="@dimen/_15ssp"
        android:layout_marginEnd="@dimen/_15ssp"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:id="@+id/profile_account_profile_image"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_margin="@dimen/_2sdp"
            android:layout_gravity="center"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="@dimen/_18sdp">

            <ImageView
                android:id="@+id/profile_img"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:contentDescription="@string/businessProfileAccountItemChildImage"
                android:scaleType="fitXY"
                android:src="@drawable/ic_add_circle"
                android:visibility="gone" />

            <TextView
                android:id="@+id/b_imageTextTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/opensans_bold"
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_20sdp"
                android:visibility="visible" />

        </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_weight="0.3"
            android:fontFamily="@font/opensans_semibold"
            android:padding="@dimen/_10sdp"
            android:text=""
            android:textColor="@color/primary_black"
            android:textSize="@dimen/_11ssp" />

        <TextView
            android:id="@+id/statusTV"
            android:layout_width="@dimen/_68sdp"
            android:layout_height="@dimen/_14sdp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/txn_pending_bg"
            android:fontFamily="@font/opensans_bold"
            android:gravity="center"
            android:text=""
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:textColor="@color/orange_status"
            android:visibility="gone"
            android:textSize="@dimen/_7ssp" />

        <ImageView
            android:id="@+id/tickIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:tint="@color/primary_green"
            android:layout_gravity="center"
            android:visibility="gone"
            android:layout_marginEnd="@dimen/_16sdp"
            android:contentDescription="@string/businessProfileAccountExpandDownArrow"
            android:src="@drawable/ic_custom_tick" />

        <ImageView
            android:id="@+id/arrowImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:padding="@dimen/_2sdp"
            android:layout_gravity="center"
            android:layout_marginEnd="@dimen/_10sdp"
            android:contentDescription="@string/businessProfileAccountExpandDownArrow"
            android:src="@drawable/ic_down" />

    </LinearLayout>

</LinearLayout>

