<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.business.MerchantTransactionDetailsActivity">

    <include
        android:id="@+id/MTDservicefee"
        layout="@layout/activity_monthly_service_fee"
        android:visibility="gone" />
    <include
        android:id="@+id/MTDsalesorder"
        layout="@layout/activity_business_sales_order"
        android:visibility="gone"/>
    <include
        android:id="@+id/MTDrefund"
        layout="@layout/activity_business_refund"
        android:visibility="gone"/>
    <include
        android:id="@+id/MTDmerchantPayout"
        layout="@layout/activity_merchant_payout"
        android:visibility="gone" />

    <include
        android:id="@+id/MTReserveRelease"
        layout="@layout/activity_reserve_payout"
        android:visibility="gone" />

</androidx.constraintlayout.widget.ConstraintLayout>