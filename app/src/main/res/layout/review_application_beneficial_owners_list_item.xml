<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_16sdp"
        app:cardCornerRadius="@dimen/_10sdp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingLeft="@dimen/_11sdp"
            android:paddingRight="@dimen/_11sdp">

            <TextView
                android:id="@+id/number_bo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_11sdp"
                android:background="@drawable/bg_inactive_color"
                android:fontFamily="@font/opensans_regular"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:text="Beneficial Owner 1"
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_11ssp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginBottom="@dimen/_1sdp"
                android:fontFamily="@font/opensans_regular"
                android:text="First Name"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/_11ssp" />

            <TextView
                android:id="@+id/firstname_bo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7sdp"
                android:fontFamily="@font/opensans_semibold"
                android:text=""
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_12ssp" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:background="@color/light_gray"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1sdp"
                android:fontFamily="@font/opensans_regular"
                android:text="Last Name"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/_11ssp" />

            <TextView
                android:id="@+id/lastname_bo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7sdp"
                android:fontFamily="@font/opensans_semibold"
                android:text=""
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_12ssp" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:background="@color/light_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1sdp"
                android:fontFamily="@font/opensans_regular"
                android:text="SSN"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/_11ssp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/ssn_bo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:layout_weight="1"
                    android:fontFamily="@font/opensans_semibold"
                    android:textColor="@color/primary_black"
                    android:textSize="@dimen/_12ssp" />

                <ImageView
                    android:id="@+id/llEIN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:paddingTop="@dimen/_3sdp"
                    android:paddingBottom="@dimen/_3sdp"
                    android:background="@drawable/ic_eyeclose" />
            </LinearLayout>

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:background="@color/light_gray"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1sdp"
                android:fontFamily="@font/opensans_regular"
                android:text="Date of Birth"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/_11ssp" />

            <TextView
                android:id="@+id/dob_bo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7sdp"
                android:fontFamily="@font/opensans_semibold"
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_12ssp" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:background="@color/light_gray"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1sdp"
                android:fontFamily="@font/opensans_regular"
                android:text="Ownership %"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/_11ssp" />

            <TextView
                android:id="@+id/ownership_bo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7sdp"
                android:fontFamily="@font/opensans_semibold"
                android:textColor="@color/primary_black"
                android:textSize="@dimen/_12ssp" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:background="@color/light_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:id="@+id/llUploadDocument"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14sdp"
                android:background="@drawable/bg_white_color_light_border"
                android:paddingLeft="@dimen/_8sdp"
                android:paddingTop="@dimen/_13sdp"
                android:paddingBottom="@dimen/_14sdp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/_6sdp"
                    android:src="@drawable/ic_upload" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:id="@+id/bo_uploaded_method"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1sdp"
                        android:fontFamily="@font/opensans_semibold"
                        android:text="Uploaded [selected method]"
                        android:textColor="@color/primary_color"
                        android:textSize="@dimen/_10ssp" />

                    <TextView
                        android:id="@+id/bo_uploaded_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:text="Uploaded on 6/11/2020"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/_8ssp" />

                </LinearLayout>

                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginStart="5dp"
                    android:background="@color/viewcolor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:fontFamily="@font/opensans_semibold"
                    android:gravity="center"
                    android:text="View"
                    android:textColor="@color/dark_grey"
                    android:textSize="@dimen/_10ssp" />
            </LinearLayout>




        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>