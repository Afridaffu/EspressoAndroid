                        android:textCursorDrawable="@drawable/cursor_color"
                        android:textSize="@dimen/_12ssp" />

                </com.google.android.material.textfield.TextInputLayout>

                <LinearLayout
                    android:id="@+id/zipcodeErrorLL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/zipcodeTIL"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="@dimen/_12sdp"
                        android:layout_height="@dimen/_12sdp"
                        android:src="@drawable/ic_erro" />

                    <TextView
                        android:id="@+id/zipcodeErrorTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:fontFamily="@font/opensans_regular"
                        android:text=""
                        android:textColor="@color/error_red"
                        android:textSize="@dimen/_10ssp" />
                </LinearLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/countryTIL"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20sp"
                    android:hint="@string/country"
                    android:textColorHint="@color/dark_grey"
                    app:boxBackgroundColor="@color/boxbackgroundgray"
                    app:boxCornerRadiusBottomEnd="@dimen/_10sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_10sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_10sdp"
                    app:boxCornerRadiusTopStart="@dimen/_10sdp"
                    app:boxStrokeColor="@color/primary_color"
                    app:expandedHintEnabled="false"
                    app:hintTextColor="@color/black">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/countryET"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:editable="false"
                        android:enabled="false"
                        android:focusable="false"
                        android:fontFamily="@font/opensans_semibold"
                        android:hint="@string/countryhint"
                        android:imeOptions="actionDone"
                        android:padding="@dimen/_12sdp"
                        android:text="@string/countryhint"
                        android:textColor="@color/dark_grey"
                        android:textColorHint="@color/light_gray"
                        android:textSize="@dimen/_12sdp" />

                </com.google.android.material.textfield.TextInputLayout>


                <androidx.cardview.widget.CardView
                    android:id="@+id/submitBtn"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_43sdp"
                    android:layout_marginTop="@dimen/_22sdp"
                    app:cardBackgroundColor="@color/inactive_color"
                    app:cardCornerRadius="@dimen/_21sdp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:fontFamily="@font/opensans_bold"
                        android:text="@string/submitbtn"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_12ssp" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/btnExit"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_43sdp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="@dimen/_26sdp"
                    android:background="@drawable/exitbtnbackgroundcolor"
                    android:fontFamily="@font/opensans_bold"
                    android:paddingTop="@dimen/_14sdp"
                    android:text="@string/exitbtn"
                    android:textAlignment="center"
                    android:textColor="@color/primary_color"
                    android:textSize="@dimen/_12ssp" />


            </LinearLayout>

        </ScrollView>


    </RelativeLayout>


</LinearLayout>