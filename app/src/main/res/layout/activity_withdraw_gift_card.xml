                            android:textSize="@dimen/_11ssp"
                            android:textColor="@color/primary_black"
                            android:fontFamily="@font/opensans_regular"
                            />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                >
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_11sdp"
                                    android:text="tempus..."
                                    android:textColor="@color/primary_black"
                                    android:textSize="@dimen/_11sdp"
                                    android:fontFamily="@font/opensans_regular"
                                    />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="@dimen/_11sdp"
                                    android:layout_weight="1"
                                    android:text="@string/view_all"
                                    android:textColor="#00a6a2"
                                    android:textSize="@dimen/_11sdp"
                                    android:fontFamily="@font/opensans_semibold"
                                    android:layout_marginBottom="@dimen/_16sdp"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_11sdp"
                        android:layout_marginRight="@dimen/_11sdp"
                        app:cardCornerRadius="@dimen/_10sdp"
                        >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="@dimen/_11sdp"
                            >
                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14sdp">

                                <LinearLayout
                                    android:id="@+id/hintdHolderll"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:background="@drawable/outline_box_unfocused"
                                    android:paddingTop="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_10sdp"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_marginStart="@dimen/_15sdp"
                                        android:gravity="center"
                                        android:orientation="horizontal">
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/opensans_semibold"
                                            android:layout_marginLeft="@dimen/_2sdp"
                                            android:text="$"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_14ssp" />

                                        <EditText
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/opensans_semibold"
                                            android:hint="$2.00 - $10,000"
                                            android:inputType="numberDecimal"
                                            android:background="@color/white"
                                            android:textColor="@color/primary_black"
                                            android:textSize="@dimen/_12ssp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/hintTVV"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="12dp"
                                    android:background="@android:color/white"
                                    android:fontFamily="@font/opensans_semibold"
                                    android:paddingStart="4dp"
                                    android:paddingEnd="4dp"
                                    android:text="Amount"
                                    android:textColor="@color/primary_black"
                                    android:textSize="@dimen/_11ssp"
                                    app:layout_constraintBottom_toTopOf="@+id/hintdHolderll"
                                    app:layout_constraintStart_toStartOf="@+id/hintdHolderll"
                                    app:layout_constraintTop_toTopOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14sdp">

                                <LinearLayout
                                    android:id="@+id/hintdHolderll1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:background="@drawable/outline_box_unfocused"
                                    android:paddingTop="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_10sdp"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_marginStart="@dimen/_15sdp"
                                        android:gravity="center"
                                        android:orientation="horizontal">
                                        <EditText
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/opensans_semibold"
                                            android:hint="First Name"
                                            android:background="@color/white"
                                            android:textColor="@color/primary_black"
                                            android:textSize="@dimen/_12ssp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/hintTV"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="12dp"
                                    android:background="@android:color/white"
                                    android:fontFamily="@font/opensans_semibold"
                                    android:paddingStart="4dp"
                                    android:paddingEnd="4dp"
                                    android:text="First Name"
                                    android:textColor="@color/primary_black"
                                    android:textSize="@dimen/_11ssp"
                                    app:layout_constraintBottom_toTopOf="@+id/hintdHolderll1"
                                    app:layout_constraintStart_toStartOf="@+id/hintdHolderll1"
                                    app:layout_constraintTop_toTopOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14sdp">

                                <LinearLayout
                                    android:id="@+id/hintdHolderll2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:background="@drawable/outline_box_unfocused"
                                    android:paddingTop="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_10sdp"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_marginStart="@dimen/_15sdp"
                                        android:gravity="center"
                                        android:orientation="horizontal">
                                        <EditText
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/opensans_semibold"
                                            android:hint="Last Name"
                                            android:background="@color/white"
                                            android:textColor="@color/primary_black"
                                            android:textSize="@dimen/_12ssp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/hintTV2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="12dp"
                                    android:background="@android:color/white"
                                    android:fontFamily="@font/opensans_semibold"
                                    android:paddingStart="4dp"
                                    android:paddingEnd="4dp"
                                    android:text="Last Name"
                                    android:textColor="@color/primary_black"
                                    android:textSize="@dimen/_11ssp"
                                    app:layout_constraintBottom_toTopOf="@+id/hintdHolderll2"
                                    app:layout_constraintStart_toStartOf="@+id/hintdHolderll2"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14sdp">

                                <LinearLayout
                                    android:id="@+id/hintdHolderll3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:background="@drawable/outline_box_unfocused"
                                    android:paddingTop="@dimen/_10sdp"
                                    android:paddingBottom="@dimen/_10sdp"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_marginStart="@dimen/_15sdp"
                                        android:gravity="center"
                                        android:orientation="horizontal">
                                        <EditText
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/opensans_semibold"
                                            android:hint="Coyni@example.com"
                                            android:background="@color/white"
                                            android:textColor="@color/primary_black"
                                            android:textSize="@dimen/_12ssp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/hintTV3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="12dp"
                                    android:background="@android:color/white"
                                    android:fontFamily="@font/opensans_semibold"
                                    android:paddingStart="4dp"
                                    android:paddingEnd="4dp"
                                    android:text="Email"
                                    android:textColor="@color/primary_black"
                                    android:textSize="@dimen/_11ssp"
                                    app:layout_constraintBottom_toTopOf="@+id/hintdHolderll3"
                                    app:layout_constraintStart_toStartOf="@+id/hintdHolderll3"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/nextCV"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        app:cardBackgroundColor="@color/inactive_color"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:gravity="center"
                        app:cardCornerRadius="@dimen/_20sdp"
                        android:layout_marginTop="@dimen/_43sdp"
                        android:layout_marginBottom="@dimen/_28sdp"
                        >
                        <TextView
                            android:id="@+id/home"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Purchase"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:textSize="@dimen/_12ssp"
                            android:fontFamily="@font/opensans_bold"
                            android:layout_gravity="center"
                            />
                    </androidx.cardview.widget.CardView>

                    </LinearLayout>
                </ScrollView>
</LinearLayout>